<script>
   function alertMess(text, sic) {
      const msgHtml = `
         <div data-v-1dcba851="" class="msg">
            <div data-v-1dcba851="" class="msg-content v-enter-active v-enter-to">${text}</div>
         </div>
      `;
      $('body').append(msgHtml);
      setTimeout(() => {
         $('.msg .msg-content').removeClass('v-enter-active v-enter-to').addClass('v-leave-active v-leave-to');
         setTimeout(() => {
            $('.msg').remove();
         }, 100);
         sic.removeClass('block-click');
      }, 1000);
   }

   async function fetchUserInfo() {
      try {
         const response = await fetch('/api/webapi/GetUserInfo');
         const data = await response.json();
         if (data && data.data) {
            const selectedAvatars = Number(data.data.avator) || 0; 
            const avatarItems = document.querySelectorAll('.van-grid-item');
            avatarItems.forEach(item => {
               const imgTag = item.querySelector('img');
               if (imgTag) {
                  imgTag.classList.remove('active');
               }
               const hiddenDiv = item.querySelector('div[style*="display: block"]');
               if (hiddenDiv) {
                  hiddenDiv.style.display = 'none';
               }
            });

            if (avatarItems.length > data.data.avator) {
               const targetItem = avatarItems[selectedAvatars]; 
               const imgTag = targetItem.querySelector('img');
               if (imgTag) {
                  imgTag.classList.add('active');
               }

               const hiddenDiv = targetItem.querySelector('div[style*="display: none"]');
               if (hiddenDiv) {
                  hiddenDiv.style.display = 'block';
               }
            }
         }
      } catch (error) {
         console.error('Error fetching user info:', error);
      }
   }

   fetchUserInfo();

   function updateAvatarSelection(event) {
      const avatarItems = document.querySelectorAll('.van-grid-item');

      avatarItems.forEach(el => {
         const img = el.querySelector('img');
         if (img) img.classList.remove('active');

         const hiddenDiv = el.querySelector('div[style*="display: block"]');
         if (hiddenDiv) hiddenDiv.style.display = 'none';
      });

      const img = event.currentTarget.querySelector('img');
      if (img) img.classList.add('active');

      const hiddenDiv = event.currentTarget.querySelector('div[style*="display: none"]');
      if (hiddenDiv) hiddenDiv.style.display = 'block';

      const selectedPosition = [...avatarItems].indexOf(event.currentTarget);

      if (confirm("Are you sure you want to update the avatar?")) {
         updateAvatarAPI(selectedPosition);
      } else {
         resetAvatarState(event);
      }
   }

   function updateAvatarAPI(selectedPosition) {

      fetch('/api/webapi/update-avator', {
         method: 'POST',
         headers: {
            'Content-Type': 'application/json',
         },
         body: JSON.stringify({ avatarId: selectedPosition })  
      })
         .then(response => response.json())
         .then(data => {
            console.log('Avatar updated:', data);
            alert("Your avatar has been updated successfully!");
         })
         .catch(error => {
            console.error('Error updating avatar:', error);
            alert("There was an error updating your avatar.");
         });
   }

   function resetAvatarState(event) {
      const img = event.currentTarget.querySelector('img');
      if (img) img.classList.remove('active');

      const hiddenDiv = event.currentTarget.querySelector('div[style*="display: block"]');
      if (hiddenDiv) hiddenDiv.style.display = 'none';
   }

   document.querySelectorAll('.van-grid-item').forEach(item => {
      item.onclick = updateAvatarSelection;
   });
</script>

