function showPasswordModal(id) {
    Swal.fire({
        title: 'Enter Password',
        html: `<input type="text" id="passwordInput" class="swal2-input" placeholder="Enter your password">`,
        showCancelButton: true,
        confirmButtonText: 'Submit',
        cancelButtonText: 'Cancel',
        preConfirm: () => {
            const password = Swal.getPopup().querySelector('#passwordInput').value;
            if (!password) {
                Swal.showValidationMessage('Please enter your password');
            }
            return password;
        }
    }).then((result) => {
        if (result.isConfirmed) {
            const password = result.value;
            console.log('Entered Password:', password);
            // Yahan aap password ko verify karne ya handlePurchase(id) function ko call kar sakte hain
            handlePurchase(id, password); // Aap apne handlePurchase function ko yahan call kar sakte hain
        }
    });
}


  function handlePurchase(id, password) {
        fetch(`/api/webapi/purchase-plan/${id}`, {
            method: 'POST', // or 'POST' if needed
            headers: {
