        function copyToClipboard(e) {
    e.preventDefault();
    
    // Get the data-clipboard-text attribute value
    const textToCopy = e.currentTarget.getAttribute('data-clipboard-text');
    
    // Copy the text to the clipboard
    navigator.clipboard.writeText(textToCopy).then(() => {
        // Show a success message (you can modify this to fit your alert system)
        alertMess('Copy successful');
    }).catch(err => {
        console.error('Failed to copy text: ', err);
    });
}

       
