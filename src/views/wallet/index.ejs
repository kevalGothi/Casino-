        $.ajax({
            type: "GET",
            url: "/api/webapi/GetUserInfo",
            dataType: "json",
            success: function (response) {
                console.log(JSON.stringify(response))
                let user = response.data;
                $('.wallet-user .name').text(user.name_user);
                         
                $('.money').text("₹" + (Number(user.winning)) );
                    $('#money_amount_display').text("₹" + (Number(user.winning)) );
                    $('#money_amount_display_wallet').text("₹" + (Number(user.money_user)) );
                    $('#total_withdraw_amount_display').text("₹" + user.winning + ".00");
                    $('#total_deposit_amount_display').text("₹" + user.winning + ".00");
                    
                    let total = (user.winning);
                    // let winPercentage = (user.winning / total) * 100;
                    let gamePercentage = (user.winning / total) * 100;
                    
                    // $('#total_deposit_amount_per').text(Math.floor(winPercentage) + "%");
                    $('#total_withdraw_amount_per').text(Math.floor(gamePercentage) + "%");

                    let circumference = 3140;

                    // let winDashArray = (winPercentage / 100) * circumference; // Win Amount ke liye dasharray value
                    let gameDashArray = (gamePercentage / 100) * circumference; // Game Amount ke liye dasharray value

                    // Update stroke-dasharray
                    // document.getElementById('total_deposit_amount_pie').style.strokeDasharray = `${winDashArray}px, ${circumference}px`;
                    document.getElementById('total_withdraw_amount_pie').style.strokeDasharray = `${gameDashArray}px, ${circumference}px`;
               
            }
        });
