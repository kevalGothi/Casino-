    <script>
        function alertMess(text, sic) {
            $('body').append(
                `
                <div data-v-1dcba851="" class="msg">
                    <div data-v-1dcba851="" class="msg-content v-enter-active v-enter-to" style=""> ${text} </div>
                </div>
                `
            );
            setTimeout(() => {
                $('.msg .msg-content').removeClass('v-enter-active v-enter-to');
                $('.msg .msg-content').addClass('v-leave-active v-leave-to');
                setTimeout(() => {
                    $('.msg').remove();
                }, 100);
                sic.removeClass('block-click');
            }, 1000);
        }
       
      $('#rebate').click(function(e) {
                e.preventDefault();
                const netAmount = $('#netAmount').val();
                const bonus = $('#bonus').text();
                const accumulatedBonus = $('#accumulatedBonus').text();
                $.ajax({
                    type: 'POST',
                    url: '/api/webapi/rebate-collect',
                    data: JSON.stringify({ netAmount: parseFloat(netAmount), bonus: parseFloat(bonus) }),
                    contentType: 'application/json',
                    success: function(response) {
                        alertMess(response.message, $('#attendance-button'));
                        if (response.status) {
                                $('#accumulatedBonus').text(bonus);
                                $('#bonus').text(0);
                            }
                    },
                    error: function(xhr, status, error) {
                        alertMess('Failed to process attendance bonus', $('#attendance-button'));
                    }
                });
            });
            
          
           $.ajax({
    type: 'GET',
    url: '/api/webapi/rebate-fetch',
    success: function(response) {
        console.log('Attendance Info Response:', response); 
        
        // Check if the status is true
        if (response.status) {
            const amount = parseFloat(response.data.amount); // Accessing amount from response

            if (!isNaN(amount)) {
                // Display the amount in the #bonus element
                $('#bonus').text(amount.toFixed(2));
                
                // Set the value of the #netAmount input field
                $('#netAmount').val(amount.toFixed(2)); // Using .val() instead of .value
            } else {
                console.log('Invalid amount:', response.data.amount);
            }
        } else {
            console.log('Failed to fetch rebate info');
        }
    },
    error: function(xhr, status, error) {
        alert('Failed to fetch rebate info');
    }
});

           
        
    </script>
